{
  "name": "lireddit",
  "version": "1.0.0",
  "description": "Node.js example server starter project",
  "main": "src/index.ts",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js --enable-source-maps",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "dev2": "nodemon --exec ts-node src/index.ts"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.4.4",
    "@mikro-orm/core": "^4.4.4",
    "@mikro-orm/migrations": "^4.4.4",
    "@mikro-orm/postgresql": "^4.4.4",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "nodemon": "1.18.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": ["./src/mikro-orm.config.ts", "./dist/mikro-orm.config.js"]
  },
  "keywords": []
}
